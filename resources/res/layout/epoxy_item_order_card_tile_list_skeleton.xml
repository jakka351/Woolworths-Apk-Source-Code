<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/epoxy_item_order_card_tile_list_skeleton_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/half_default_padding"
    android:layout_marginBottom="@dimen/half_default_padding"
    android:layout_marginStart="@dimen/default_padding"
    android:layout_marginEnd="@dimen/default_padding">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:padding="@dimen/default_padding"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/status_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/shape_circle_fill"
            android:importantForAccessibility="no"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <View
            android:id="@+id/status_label"
            android:background="@drawable/background_skeleton_box_small_radius"
            android:layout_width="156dp"
            android:layout_height="12dp"
            android:layout_marginStart="@dimen/three_quarter_default_margin"
            app:layout_constraintEnd_toStartOf="@+id/price_label"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toEndOf="@+id/status_icon"
            app:layout_constraintTop_toTopOf="parent"/>
        <View
            android:id="@+id/price_label"
            android:background="@drawable/background_skeleton_box_large_radius"
            android:paddingTop="@dimen/quarter_default_padding"
            android:paddingBottom="@dimen/quarter_default_padding"
            android:layout_width="56dp"
            android:layout_height="24dp"
            android:paddingStart="@dimen/half_default_padding"
            android:paddingEnd="@dimen/half_default_padding"
            app:layout_constraintBottom_toBottomOf="@+id/status_label"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/status_label"
            app:layout_constraintTop_toTopOf="@+id/status_label"/>
        <View
            android:id="@+id/caption_label"
            android:background="@drawable/background_skeleton_box_small_radius"
            android:layout_width="92dp"
            android:layout_height="12dp"
            android:layout_marginTop="@dimen/half_default_margin"
            app:layout_constraintEnd_toStartOf="@+id/price_label"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/status_label"
            app:layout_constraintTop_toBottomOf="@+id/status_label"/>
        <View
            android:id="@+id/address_label"
            android:background="@drawable/background_skeleton_box_small_radius"
            android:layout_width="136dp"
            android:layout_height="12dp"
            android:layout_marginTop="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/status_label"
            app:layout_constraintTop_toBottomOf="@+id/caption_label"/>
        <View
            android:textAppearance="@style/TextAppearance.Body.Medium"
            android:id="@+id/estimated_arrival_time"
            android:background="@drawable/background_skeleton_box_small_radius"
            android:layout_width="200dp"
            android:layout_height="12dp"
            android:layout_marginTop="@dimen/half_default_margin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/status_label"
            app:layout_constraintTop_toBottomOf="@+id/address_label"/>
        <View
            android:id="@+id/chosen_window_date"
            android:background="@drawable/background_skeleton_box_small_radius"
            android:layout_width="92dp"
            android:layout_height="12dp"
            android:layout_marginTop="@dimen/half_default_margin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/status_label"
            app:layout_constraintTop_toBottomOf="@+id/estimated_arrival_time"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
