<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/connection_error_layout"
    android:tag="layout/include_connection_error_0"
    style="@style/ErrorLayoutStyle">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="0x7f080269"
        android:importantForAccessibility="no"/>
    <TextView
        android:id="@+id/connection_error_text_view"
        android:text="@string/search_connection_error"
        style="@style/Subhead.ErrorLayoutText"/>
    <TextView
        android:id="@+id/connection_error_try_again"
        style="@style/Button.ErrorLayoutButton"/>
</LinearLayout>
