<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/epoxy_item_shopping_list_product_buttons_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/product_unavailable_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/include_brand_label"/>
    <Button
        android:id="@+id/availability_info_button"
        android:tag="binding_1"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="4dp"
        android:singleLine="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_unavailable_text_view"
        app:layout_constraintWidth_max="@dimen/product_see_in_store_button_width"
        style="@style/Button.OutlinedButton.Wow.Info"/>
    <Button
        android:id="@+id/product_unavailable_view_similar_button"
        android:tag="binding_2"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="4dp"
        android:text="@string/shop_lists_product_item_view_similar"
        android:singleLine="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_unavailable_text_view"
        app:layout_constraintWidth_max="@dimen/product_list_button_width"
        style="?attr/materialButtonOutlinedStyle"/>
    <Button
        android:id="@+id/add_button"
        android:tag="binding_3"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:singleLine="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="@dimen/product_list_button_width"/>
</androidx.constraintlayout.widget.ConstraintLayout>
