<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/epoxy_include_rewards_empty_state_0"
    android:padding="@dimen/triple_default_padding"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/message_image_view"
        android:tag="binding_1"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="@dimen/default_margin"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/message_image_url_view"
        app:layout_constraintEnd_toEndOf="@+id/message_image_url_view"
        app:layout_constraintStart_toStartOf="@+id/message_image_url_view"
        app:layout_constraintTop_toTopOf="@+id/message_image_url_view"/>
    <ImageView
        android:id="@+id/message_image_url_view"
        android:tag="binding_2"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginBottom="@dimen/default_margin"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toTopOf="@+id/title_text_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Body.Medium"
        android:id="@+id/title_text_view"
        android:tag="binding_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/message_text_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/message_image_url_view"/>
    <TextView
        android:textColor="?android:attr/textColorSecondary"
        android:gravity="center"
        android:id="@+id/message_text_view"
        android:tag="binding_4"
        android:padding="@dimen/half_default_padding"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/action_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_text_view"/>
    <Button
        android:id="@+id/action_button"
        android:tag="binding_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/message_text_view"
        style="@style/Button.Primary.Flat"/>
</androidx.constraintlayout.widget.ConstraintLayout>
