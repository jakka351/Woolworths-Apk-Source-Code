<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/checkout_module_navigation"
    app:startDestination="@+id/checkout_details_fragment">
    <fragment
        android:label="@string/title_checkout"
        android:name="au.com.woolworths.shop.checkout.ui.details.CheckoutDetailsFragment"
        android:id="@+id/checkout_details_fragment">
        <action
            android:id="@+id/action_to_restart_checkout"
            app:destination="@+id/checkout_details_fragment"
            app:popUpTo="@+id/checkout_details_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_checkout_details_fragment_to_marketplace_pickup_dialog"
            app:destination="@+id/marketplace_pickup_dialog">
            <argument
                android:name="screen"
                app:argType="au.com.woolworths.android.onesite.analytics.Screens"
                app:nullable="false"/>
            <argument
                android:name="bottomSheetContent"
                app:argType="au.com.woolworths.foundation.shop.bottomsheet.BottomSheetContent$MarketplacePickUp"
                app:nullable="false"/>
        </action>
        <action
            android:id="@+id/action_checkout_details_fragment_to_checkout_contact_details_fragment"
            app:destination="@+id/checkout_contact_details_fragment">
            <argument
                android:name="extras"
                app:argType="au.com.woolworths.shop.checkout.ui.contactdetails.ContactDetailsExtras"/>
        </action>
    </fragment>
    <fragment
        android:label="@string/title_checkout"
        android:name="au.com.woolworths.shop.checkout.ui.product.CheckoutProductFragment"
        android:id="@+id/checkout_product_fragment">
        <action
            android:id="@+id/action_checkout_product_fragment_to_shopper_note_fragment"
            app:destination="@+id/shopper_note_fragment">
            <argument
                android:name="extras"
                app:argType="au.com.woolworths.shop.checkout.ui.shopperNote.ShopperNoteExtras"/>
        </action>
    </fragment>
    <fragment
        android:label="@string/title_checkout"
        android:name="au.com.woolworths.shop.checkout.ui.contactdetails.ContactDetailsFragment"
        android:id="@+id/checkout_contact_details_fragment">
        <argument
            android:name="extras"
            app:argType="au.com.woolworths.shop.checkout.ui.contactdetails.ContactDetailsExtras"/>
    </fragment>
    <fragment
        android:name="au.com.woolworths.shop.checkout.ui.shopperNote.ShopperNoteFragment"
        android:id="@+id/shopper_note_fragment">
        <argument
            android:name="extras"
            app:argType="au.com.woolworths.shop.checkout.ui.shopperNote.ShopperNoteExtras"/>
    </fragment>
    <fragment
        android:name="au.com.woolworths.shop.checkout.ui.fulfilmentwindows.FulfilmentWindowsFragment"
        android:id="@+id/checkout_fulfilment_windows_fragment">
        <argument
            android:name="fulfilmentWindowSource"
            android:defaultValue="CHECKOUT"
            app:argType="au.com.woolworths.shop.checkout.ui.fulfilmentwindows.FulfilmentWindowSource"/>
    </fragment>
    <fragment
        android:label="@string/title_checkout"
        android:name="au.com.woolworths.shop.checkout.ui.summary.CheckoutSummaryFragment"
        android:id="@+id/checkout_summary_fragment">
        <action
            android:id="@+id/action_summary_to_refund_bottom_sheet"
            app:destination="@+id/refund_bottom_sheet_fragment">
            <argument
                android:name="info"
                app:argType="au.com.woolworths.foundation.shop.bottomsheet.CheckoutRefundInfo"
                app:nullable="false"/>
        </action>
    </fragment>
    <dialog
        android:name="au.com.woolworths.shop.checkout.ui.summary.refund.RefundBottomSheetFragment"
        android:id="@+id/refund_bottom_sheet_fragment">
        <argument
            android:name="info"
            app:argType="au.com.woolworths.foundation.shop.bottomsheet.CheckoutRefundInfo"
            app:nullable="false"/>
    </dialog>
    <dialog
        android:name="au.com.woolworths.shop.checkout.ui.content.bottomsheet.MarketplacePickupBottomSheetFragment"
        android:id="@+id/marketplace_pickup_dialog">
        <argument
            android:name="screen"
            app:argType="au.com.woolworths.android.onesite.analytics.Screens"/>
        <argument
            android:name="bottomSheetContent"
            app:argType="au.com.woolworths.foundation.shop.bottomsheet.BottomSheetContent$MarketplacePickUp"/>
    </dialog>
</navigation>
